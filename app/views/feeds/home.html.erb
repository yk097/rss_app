<div class="container">
  <div class="feedcontainer">
    <% @rssfeeds.each do |rssfeed| %>
      <div class="contentsbox">
        <div class="sitename">
          <%= link_to rssfeed["channel"]["title"], "#" %>
        </div>
        <div class="rssitems">
          <ul>
            <% rssfeed["item"].first(5).each do |feed| %>
              <li><%= link_to feed["title"], feed["link"], target: :_blank, rel: "noopener noreferrer" %>
            <% end %>
          </ul>
        </div>
      </div>
    <% end %>
  </div>
</div>

